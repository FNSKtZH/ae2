{
  "version": 2,
  "name": "ae2",
  "env": {
    "PGUSER": "@pguser",
    "PGHOST": "@pghost",
    "PGPASSWORD": "@pgpassword",
    "PGDATABASE": "@pgdatabase",
    "PGPORT": "@pgport"
  },
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/"
    },
    {
      "source": "/Arten/(.*)",
      "destination": "/Arten"
    },
    {
      "source": "/Export",
      "destination": "/Export"
    },
    {
      "source": "/Export/(.*)",
      "destination": "/Export"
    },
    {
      "source": "/Benutzer",
      "destination": "/Benutzer"
    },
    {
      "source": "/Benutzer/(.*)",
      "destination": "/Benutzer"
    },
    {
      "source": "/Organisationen",
      "destination": "/Organisationen"
    },
    {
      "source": "/Organisationen/(.*)",
      "destination": "/Organisationen"
    },
    {
      "source": "/graphiql",
      "destination": "/graphiql"
    },
    {
      "source": "/graphiql/(.*)",
      "destination": "/graphiql"
    },
    {
      "source": "/graphql",
      "destination": "https://api.artdaten.ch/graphql"
    },
    {
      "source": "/artendb/_design/artendb/_list/export_evab/evab_arten",
      "destination": "https://api.artdaten.ch/artendb/_design/artendb/_list/export_evab/evab_arten"
    },
    {
      "source": "/api/evab/arten",
      "destination": "https://api.artdaten.ch/api/evab/arten"
    },
    {
      "source": "/artendb/_design/artendb/_list/export_alt_mit_synonymen/alt_arten_mit_synonymen",
      "destination": "https://api.artdaten.ch/artendb/_design/artendb/_list/export_alt_mit_synonymen/alt_arten_mit_synonymen"
    },
    {
      "source": "/api/alt",
      "destination": "https://api.artdaten.ch/api/alt"
    },
    {
      "source": "/artendb/_design/artendb/_list/export_alt_mit_synonymen_standardfelder/alt_arten_mit_synonymen",
      "destination": "https://api.artdaten.ch/artendb/_design/artendb/_list/export_alt_mit_synonymen_standardfelder/alt_arten_mit_synonymen"
    }
  ]
}