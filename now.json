{
  "version": 2,
  "name": "ae2",
  "env": {
    "DBUSER": "@dbuser",
    "DBPASS": "@dbpass"
  },
  "routes": [
    {
      "src": "/index.html",
      "dest": "/"
    },
    {
      "src": "/Arten/.*",
      "dest": "/Arten"
    },
    {
      "src": "/Export",
      "dest": "/Export"
    },
    {
      "src": "/Export/.*",
      "dest": "/Export"
    },
    {
      "src": "/graphiql",
      "dest": "/graphiql"
    },
    {
      "src": "/graphiql/.*",
      "dest": "/graphiql"
    },
    {
      "src": "/graphql",
      "dest": "https://api.artdaten.ch/graphql"
    },
    {
      "src": "/artendb/.*",
      "dest": "alt"
    }
  ]
}