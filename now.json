{
  "version": 2,
  "name": "ae2",
  "env": {
    "DBUSER": "@dbuser",
    "DBPASS": "@dbpass"
  },
  "routes": [
    {
      "src": "/index.html",
      "dest": "/"
    },
    {
      "src": "/Arten/.*",
      "dest": "/Arten"
    },
    {
      "src": "/Export",
      "dest": "/Export"
    },
    {
      "src": "/Export/.*",
      "dest": "/Export"
    },
    {
      "src": "/graphiql",
      "dest": "/graphiql"
    },
    {
      "src": "/graphiql/.*",
      "dest": "/graphiql"
    },
    {
      "src": "/graphql",
      "dest": "https://api.artdaten.ch/graphql"
    },
    {
      "src": "/artendb/_design/artendb/_list/export_alt_mit_synonymen_standardfelder/alt_arten_mit_synonymen",
      "dest": "https://api.artdaten.ch"
    },
    {
      "src": "/artendb/(.*)",
      "dest": "https://api.artdaten.ch"
    },
    {
      "src": "/api/evab/arten",
      "dest": "https://api.artdaten.ch"
    },
    {
      "src": "/api/alt",
      "dest": "https://api.artdaten.ch"
    }
  ]
}