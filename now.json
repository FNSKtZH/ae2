{
  "version": 2,
  "name": "ae2",
  "env": {
    "PGUSER": "@pguser",
    "PGHOST": "@pghost",
    "PGPASSWORD": "@pgpassword",
    "PGDATABASE": "@pgdatabase",
    "PGPORT": "@pgport"
  },
  "routes": [
    {
      "src": "/index.html",
      "dest": "/"
    },
    {
      "src": "/Arten/(.*)",
      "dest": "/Arten"
    },
    {
      "src": "/Export",
      "dest": "/Export"
    },
    {
      "src": "/Export/(.*)",
      "dest": "/Export"
    },
    {
      "src": "/Benutzer",
      "dest": "/Benutzer"
    },
    {
      "src": "/Benutzer/(.*)",
      "dest": "/Benutzer"
    },
    {
      "src": "/Organisationen",
      "dest": "/Organisationen"
    },
    {
      "src": "/Organisationen/(.*)",
      "dest": "/Organisationen"
    },
    {
      "src": "/graphiql",
      "dest": "/graphiql"
    },
    {
      "src": "/graphiql/(.*)",
      "dest": "/graphiql"
    },
    {
      "src": "/graphql",
      "dest": "https://api.artdaten.ch/graphql"
    },
    {
      "src": "/artendb/_design/artendb/_list/export_evab/evab_arten",
      "dest": "/api/evab/arten.js"
    },
    {
      "src": "/artendb/_design/artendb/_list/export_alt_mit_synonymen/alt_arten_mit_synonymen",
      "dest": "/api/alt.js"
    },
    {
      "src": "/artendb/_design/artendb/_list/export_alt_mit_synonymen_standardfelder/alt_arten_mit_synonymen",
      "dest": "/api/alt.js"
    }
  ]
}